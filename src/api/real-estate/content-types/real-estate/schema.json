{
  "kind": "collectionType",
  "collectionName": "real_estates",
  "info": {
    "singularName": "real-estate",
    "pluralName": "real-estates",
    "displayName": "Real-estate"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "location": {
      "type": "text",
      "maxLength": 100,
      "required": true
    },
    "price": {
      "type": "biginteger",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "apartment",
        "floor",
        "shop",
        "showroom",
        "office",
        "villa",
        "warehouse",
        "workshop"
      ]
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "real_estate"
    },
    "condition": {
      "type": "enumeration",
      "required": true,
      "default": "sale",
      "enum": [
        "rent",
        "sale"
      ]
    },
    "city": {
      "type": "enumeration",
      "required": true,
      "default": "riyadh",
      "enum": [
        "riyadh",
        "jeddah"
      ]
    },
    "youtube_video_link": {
      "type": "text",
      "required": true
    },
    "license_number": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "size": {
      "type": "biginteger",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "google_map_link": {
      "type": "text",
      "required": true
    }
  }
}
