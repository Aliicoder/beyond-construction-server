{
  "kind": "collectionType",
  "collectionName": "real_estates",
  "info": {
    "singularName": "real-estate",
    "pluralName": "real-estates",
    "displayName": "real-estate"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "location": {
      "type": "text",
      "maxLength": 30,
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "width": {
      "type": "integer",
      "required": true
    },
    "height": {
      "type": "integer",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "apartment",
        "building",
        "villa"
      ]
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
